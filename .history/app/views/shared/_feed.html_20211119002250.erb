<% if @feed_items.any? && @search.any? %>
  <ol class="microposts">
    <%= render @feed_items %>
  </ol>
  <%= will_paginate @feed_items, params:{ controller: :static_pages, action: :home } %>
<% end %>

<% if @feed_items.any? && @search.nil? %>
  <ol class="microposts">
    <%= render @feed_items %>
  </ol>
  <%= will_paginate @feed_items, params:{ controller: :static_pages, action: :home } %>
<% end %>