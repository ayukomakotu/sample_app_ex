<%= form_with(url: search_users_path, scope: :search, #params[:search]に保存
                                      method: :search, #searchアクションを指定
                                      local: true) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name, class: 'form-control' %>

    <%= f.submit "search user", class: "btn btn-primary" %>
    
<% end %>
